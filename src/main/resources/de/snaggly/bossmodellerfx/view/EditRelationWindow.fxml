<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="windowAnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.snaggly.bossmodellerfx.view.controller.EditRelationWindowController">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" text="Relation bearbeiten" VBox.vgrow="NEVER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Tabelle A" HBox.hgrow="NEVER">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="tableAEntityCmboBox" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minWidth="150.0" onAction="#updateConnectionLine" HBox.hgrow="ALWAYS" />
                  <VBox spacing="3.0">
                     <children>
                        <RadioButton fx:id="radioBtnPolyA1" mnemonicParsing="false" onAction="#updateConnectionLine" selected="true" text="1">
                           <toggleGroup>
                              <ToggleGroup fx:id="PolynomA" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radioBtnPolyAN" mnemonicParsing="false" onAction="#updateConnectionLine" text="N" toggleGroup="$PolynomA" />
                     </children>
                  </VBox>
                  <VBox spacing="3.0">
                     <children>
                        <RadioButton fx:id="radioBtnObligationACan" mnemonicParsing="false" onAction="#updateConnectionLine" selected="true" text="Kann">
                           <toggleGroup>
                              <ToggleGroup fx:id="ObligationA" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radioBtnObligationAMust" mnemonicParsing="false" onAction="#updateConnectionLine" text="Muss" toggleGroup="$ObligationA" />
                     </children>
                  </VBox>
                  <CheckBox fx:id="tableAIsWeakChkBox" mnemonicParsing="false" onAction="#onTableAIsWeakCheckBoxAction" text="Ist Schwach" HBox.hgrow="NEVER" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER" layoutX="30.0" layoutY="213.0" spacing="10.0">
               <children>
                  <Label text="Tabelle B" HBox.hgrow="NEVER">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="tableBEntityCmboBox" maxWidth="1.7976931348623157E308" minWidth="150.0" onAction="#updateConnectionLine" HBox.hgrow="ALWAYS" />
                  <VBox spacing="3.0">
                     <children>
                        <RadioButton fx:id="radioBtnPolyB1" mnemonicParsing="false" onAction="#updateConnectionLine" selected="true" text="1">
                           <toggleGroup>
                              <ToggleGroup fx:id="PolynomB" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radioBtnPolyBN" mnemonicParsing="false" onAction="#updateConnectionLine" text="N" toggleGroup="$PolynomB" />
                     </children>
                  </VBox>
                  <VBox spacing="3.0">
                     <children>
                        <RadioButton fx:id="radioBtnObligationBCan" mnemonicParsing="false" onAction="#updateConnectionLine" selected="true" text="Kann">
                           <toggleGroup>
                              <ToggleGroup fx:id="ObligationB" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radioBtnObligationBMust" mnemonicParsing="false" onAction="#updateConnectionLine" text="Muss" toggleGroup="$ObligationB" />
                     </children>
                  </VBox>
                  <CheckBox fx:id="tableBIsWeakChkBox" mnemonicParsing="false" onAction="#onTableBIsWeakCheckBoxAction" text="Ist Schwach" HBox.hgrow="NEVER" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox fx:id="examplePane" alignment="CENTER" fillHeight="false" spacing="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" spacing="15.0">
               <children>
                  <Button layoutX="279.0" layoutY="10.0" mnemonicParsing="false" onAction="#closeWindow" text="Abbrechen" />
                  <Button mnemonicParsing="false" onAction="#save" text="Speichern" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Button mnemonicParsing="false" onAction="#showInfo" text="Info" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="20.0" />
   </children>
</AnchorPane>
